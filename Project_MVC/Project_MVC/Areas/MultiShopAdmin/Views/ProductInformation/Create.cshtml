@model ProductInformation
@{
    ViewData["Title"] = "Create";
}


<div class="main-panel">
    <h1>Create ProductInformation</h1>
    <form class="mt-3" method="post" enctype="multipart/form-data">
    <div>
        <label >Key:</label><br />
        <input asp-for="Key" class="form-control" /><br />
        <span class="text-danger" asp-validation-for="Key"></span>
    </div>
    <div>
        <label >Desc:</label><br />
        <input asp-for="Desc" class="form-control" /><br />
        <span class="text-danger" asp-validation-for="Desc"></span>
    </div>
    <div class="mt-2">
        <button class="btn btn-success" type="submit">Create</button>
    </div>
</form>
</div>


